{
  "name": "domus",
  "version": "2.0",
  "description": "",
  "author": "Karl Malone",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "build:watch": "nest build --watch",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:unit": "jest --testPathIgnorePatterns .integration.spec.ts .e2e-spec.ts",
    "test:e2e": "cross-env NODE_ENV=test jest --config test/helpers/jest-e2e.json --runInBand --forceExit",
    "test:integration": "cross-env NODE_ENV=test jest --config test/helpers/jest-integration.json --runInBand --forceExit",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:generate": "npm run typeorm -- migration:generate -d src/infra/database/data-source.ts src/infra/database/migrations/$npm_config_name",
    "migration:run": "npm run typeorm -- migration:run -d src/infra/database/data-source.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/infra/database/data-source.ts",
    "doc": "npx compodoc -p tsconfig.json -s"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.478.0",
    "@aws-sdk/client-ses": "^3.926.0",
    "@aws-sdk/client-sesv2": "^3.846.0",
    "@aws-sdk/client-sqs": "^3.926.0",
    "@aws-sdk/s3-request-presigner": "^3.478.0",
    "@date-fns/utc": "^2.1.0",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.3",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.3",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.3",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "@scalar/nestjs-api-reference": "^1.0.8",
    "@ssut/nestjs-sqs": "^3.0.1",
    "axios": "^1.5.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cliui": "^8.0.1",
    "date-fns": "^4.1.0",
    "joi": "^18.0.1",
    "multer": "^2.0.1",
    "multer-s3": "^3.0.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "stripe": "^18.3.0",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.25",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.32",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.3",
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__template": "^7.4.4",
    "@types/babel__traverse": "^7.20.7",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.6",
    "@types/estree": "^1.0.8",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/json-schema": "^7.0.15",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.0.13",
    "@types/nodemailer": "^6.4.9",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "glob": "^11.0.3",
    "jest": "^30.0.4",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.20",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typeorm-ts-node-commonjs": "^0.3.20",
    "typescript": "^5.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "ts-node": {
    "require": [
      "tsconfig-paths/register"
    ],
    "transpileOnly": true
  }
}
